generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//yo

model Decklist {
  id       Int    @id @default(autoincrement())
  name     String
  playerId String
  cards    Json

  @@map("decklists") // mapping alla tabella gi√† usata
}
