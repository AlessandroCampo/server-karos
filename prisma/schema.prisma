generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Player {
  id        Int        @id @default(autoincrement())
  name      String
  decklists Decklist[]
}

model Decklist {
  id       Int    @id @default(autoincrement())
  name     String
  playerId Int
  cards    Json

  player Player @relation(fields: [playerId], references: [id], onDelete: Cascade)
}
